<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="Boot.mapper.UserMapper">
    <select id="queryUserList" resultType="Boot.domain.User">
       select * from user
    </select>
    <select id="findById" resultType="Boot.domain.User">
        select * from user where id = #{id}
    </select>
</mapper>